          "accept": { "kinds": ["text"], "roles": ["citations"] },
          "capacity": { "min": 0, "max": 1 },
          "style_ref": "role.citations",
          "overflow_policy": { "allow_shrink": true, "min_font_pt": 8, "on_overflow": ["shrink_font", "truncate"] },
          "z_base": 5
