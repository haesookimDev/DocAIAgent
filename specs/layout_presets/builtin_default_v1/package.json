          "accept": { "kinds": ["text"], "roles": ["title"] },
          "capacity": { "min": 1, "max": 1 },
          "style_ref": "role.title",
          "overflow_policy": { "allow_shrink": true, "min_font_pt": 24, "on_overflow": ["shrink_font", "wrap", "truncate"] },
          "z_base": 20
        },
        {
          "slot_id": "subtitle",
          "bbox": { "unit": "rel_safe", "x": 0.0, "y": 0.52, "w": 1.0, "h": 0.12 },
          "padding_in": { "t": 0.04, "r": 0.06, "b": 0.04, "l": 0.06 },
